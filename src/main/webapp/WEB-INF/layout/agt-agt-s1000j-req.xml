<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<layout desc="Agent정보 조회 요청" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="./layout.xsd">

	<field name="timestamp"		maxBytes="14"	required="Y"	type="C"	desc="요청시각" />
	<field name="userId"		maxBytes="0"	required="N"	type="C"	desc="사용자 아이디" />
	<field name="certId"		maxBytes="68"	required="N"	type="C"	desc="인증서 아이디" />
	<field name="power"			maxBytes="10"	required="N"	type="C"	desc="Agent의 on/off여부" />
	<field name="time"			maxBytes="10"	required="N"	type="C"	desc="Agent의 on/off 판단 시간(단위:분)" />
	<field name="pageNo"		maxBytes="3"	required="N"	type="C"	desc="조회 페이지 번호" />
	<field name="limit"			maxBytes="3"	required="N"	type="C"	desc="페이지당 조회 개수" />
</layout>